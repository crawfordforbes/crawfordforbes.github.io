{
  "name": "crawfordforbes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "images:build": "node scripts/optimize-images.cjs",
    "postinstall": "patch-package",
    "videos:build": "node scripts/transcode-videos.cjs",
    "types": "tsc -p tsconfig.declarations.json",
